<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.english.english_vision.mapper.CorrectWordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.english.english_vision.pojo.CorrectWord">
        <id column="id" property="id" />
        <result column="word_id" property="wordId" />
        <result column="user_id" property="userId" />
        <result column="total_times" property="totalTimes" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, word_id, user_id, total_times
    </sql>

</mapper>
